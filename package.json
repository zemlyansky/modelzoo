{
  "name": "modelzoo",
  "version": "0.0.1",
  "description": "Collection of models for TensorFlow.js",
  "main": "./dist/cjs/index.js",
  "scripts": {
    "build": "npm run build_cjs && npm run build_esm",
    "build_cjs": "tsc src/index.ts --module commonjs --target es2015 --outDir dist/cjs",
    "build_esm": "tsc src/index.ts --module esnext --outDir dist/esm",
    "watch": "nodemon --watch src --watch test --ext ts,js,json --exec \"npm run build && npm test\"",
    "test": "jest"
  },
  "author": "Anton Zemlyansky",
  "license": "MIT",
  "dependencies": {
    "@tensorflow/tfjs": "4.10.0"
  },
  "devDependencies": {
    "@tensorflow/tfjs-node": "4.10.0",
    "jest": "^29.7.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zemlyansky/modelzoo.git"
  },
  "jest": {
    "collectCoverage": false,
    "modulePathIgnorePatterns": ["temp/", "dist/", "node_modules/", "py/"],
    "testMatch": ["**/test/*_test.js"]
  }
}
